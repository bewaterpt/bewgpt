<div role="document" class="page">
  {%  if page.header %}
    <header id="site-header">
      <div class="outer-wrapper">
        {{ page.header }}
      </div>
    </header>
  {% endif %}

  {%  if page.featured %}
    <section id="featured">
      <div class="columns">
        {{ page.featured }}
      </div>
    </section>
  {% endif %}

  {% if page.breadcrumb %}
    <section id="breadcrumb">
      <div class="outer-wrapper">
        {{ page.breadcrumb }}
      </div>
    </section>
  {% endif %}

  <main role="main" class="outer-wrapper">
    <a id="main-content" tabindex="-1"></a>
    <div class="menu_btn_container">
      <div id="menuBar_btn" class="menuBar_btn">
        {# <i class="fas fa-chevron-right"></i>
        <i class="fas fa-chevron-right"></i> #}
      </div>
    </div>
    {% if page.sidebar_first %}
      <aside id="sidebar-first" role="complementary" class="sidebar">
        {{ page.sidebar_first }}
      </aside>
    {% endif %}

    {% if page.sidebar %}
      <aside id="sidebar-first" role="complementary" class="sidebar">
        {{ page.sidebar }}
      </aside>
    {% endif %}

    <section id="content" style="background-image: url({{ background_url }}); background-size: cover; background-repeat: no-repeat; background-attachment: fixed;" >
    {% if  %}

    {% if is_front %}
      <div id="footer_btn_container" class="footer_btn_container">
        <div id="footer_close" class="footer_close"> </div>
      </div>
    {% endif %}
      {{ page.content }}
    </section>

    {% if page.sidebar_second %}
      <aside id="sidebar-second" role="complementary" class="sidebar">
        {{ page.sidebar_second }}
      </aside>
    {% endif %}
  </main>

  {% if page.footer_top or page.footer_bottom %}
    <footer id="site-footer" role="contentinfo">
      {% if page.footer_top %}
        <section class="footer-top">
          {{ page.footer_top }}
        </section>
      {% endif %}

      {% if page.footer_bottom %}
        <section class="footer-bottom">
          {{ page.footer_bottom }}
        </section>
      {% endif %}
    </footer>
  {% endif %}



</div>
